FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     anisotropicBiotFoam;

startFrom       latestTime;

startTime	25;

stopAt          endTime;

endTime         10000;

deltaT          0.5;

writeControl    timeStep;

writeInterval   10;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable yes;

nonlinearity nonLinear;

wave standingWave;

libs ( /*"libSoilFvPatchFields.so"*/ "libOpenFOAM.so" "libgroovyBC.so");


