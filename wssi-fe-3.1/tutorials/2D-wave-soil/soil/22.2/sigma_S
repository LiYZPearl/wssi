FoamFile
{
    version     2.0;
    format      ascii;
    class       volSymmTensorField;
    location    "10";
    object      sigma_S;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform (0 0 0 0 0 0);

boundaryField
{
    bottom
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
    sides
    {
        type            zeroGradient;
    }
    seabed
    {
        type            zeroGradient;
    }
}


